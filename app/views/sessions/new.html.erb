<div id='name'><B>Place Cursor <u>Here</u> For More Info</B></div>
<div id='popup'>Must log in first!!</div>



<h1>Sign in</h1>

<%= form_tag sessions_path do %>
    <table>   
     <tr>  
          <td><%= label_tag :name %></td>
            <td><%= text_field_tag :name, params[:name] %>
        </tr>
        <tr>  
          <td><%= label_tag :password %></td> 
           <td><%= password_field_tag :password %>
        </tr>   
     <tr>  
          <td><%= submit_tag "Sign in" %></td>
        </tr> 
   </table>
   
   
   

  
<% end %>
